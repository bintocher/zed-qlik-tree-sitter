{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "qlik",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "bracket_field"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "comparison_operator"
        },
        {
          "type": "SYMBOL",
          "name": "arithmetic_operator"
        },
        {
          "type": "SYMBOL",
          "name": "parenthesized"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": ","
        }
      ]
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "rem_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[rR][eE][mM]"
            },
            {
              "type": "PATTERN",
              "value": "\\s[^;]*"
            },
            {
              "type": "STRING",
              "value": ";"
            }
          ]
        }
      }
    },
    "single_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "'"
          },
          {
            "type": "PATTERN",
            "value": "[^']*"
          },
          {
            "type": "STRING",
            "value": "'"
          }
        ]
      }
    },
    "double_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "[^\"]*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "bracket_field": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "["
          },
          {
            "type": "PATTERN",
            "value": "[^\\]]*"
          },
          {
            "type": "STRING",
            "value": "]"
          }
        ]
      }
    },
    "integer": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+"
      }
    },
    "float": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+\\.\\d+([eE][+-]?\\d+)?"
      }
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "macro": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "$("
          },
          {
            "type": "PATTERN",
            "value": "[^)]*"
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "comparison_operator": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "<>"
          },
          {
            "type": "STRING",
            "value": "<="
          },
          {
            "type": "STRING",
            "value": ">="
          },
          {
            "type": "STRING",
            "value": "<<"
          },
          {
            "type": "STRING",
            "value": ">>"
          },
          {
            "type": "STRING",
            "value": "<"
          },
          {
            "type": "STRING",
            "value": ">"
          },
          {
            "type": "STRING",
            "value": "="
          }
        ]
      }
    },
    "arithmetic_operator": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "&"
          },
          {
            "type": "STRING",
            "value": "+"
          },
          {
            "type": "STRING",
            "value": "-"
          },
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "parenthesized": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_item"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "function_call": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_item"
            }
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_#][a-zA-Z0-9_#.]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "SYMBOL",
      "name": "rem_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}