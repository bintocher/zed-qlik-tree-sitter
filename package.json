{
  "name": "tree-sitter-qlik",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for Qlik Sense / QlikView script language (.qvs)",
  "main": "bindings/node",
  "author": "bintocher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bintocher/zed-qlik-tree-sitter.git"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "qlik",
    "qlik-sense",
    "qlikview",
    "qvs"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.qvs",
      "file-types": [
        "qvs"
      ],
      "highlights": "queries/highlights.scm"
    }
  ],
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  }
}
